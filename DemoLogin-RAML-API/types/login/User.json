{
   "type": "object",
   "$schema": "http://json-schema.org/draft-04/schema#",
   "description": "Objeto utilizado para la validacion de los datos del usuario.",
   "properties": {
       "user": {
           "type": "string",
           "minLength": 1,
           "maxLength": 10
       },
       "password": {
           "type": "string",
           "minLength": 8,
           "maxLength": 8
       }
   },
   "required": [
       "user",
       "password"
   ]
}